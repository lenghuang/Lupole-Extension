{
  "name": "Lupole",
  "version": "1.0",
  "description": "The Alternative Spending Assistant!",
  "content_scripts": [
    {
      "matches": ["https://www.amazon.com/*", "https://www.nike.com/*"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {
      "16": "icon.png",
      "24": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "default_popup": "popup.html",
    "default_title": "Alternative Spending Assistant"
  },
  "manifest_version": 2
}
