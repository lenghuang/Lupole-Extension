{
  "name": "Lupole",
  "version": "1.0",
  "description": "The Alternative Spending Assistant!",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-3.4.1.js",
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.jpg",
    "default_popup": "popup.html",
    "default_title": "Alternative Spending Assistant"
  },
  "manifest_version": 2
}